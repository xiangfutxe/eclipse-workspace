/**********加入购物车************/
#s-actionBar-container {
    position: fixed;
    bottom: 0;
    width: 100%;
    z-index: 1;
}
section, header {
    background-color: #fff;
}
#s-actionBar-container .action-bar {
    color: #999;
    border-top: 1px solid #E5E5E5;
}
.mui-flex.align-center {
    -webkit-box-align: center;
    -webkit-align-items: center;
    -ms-flex-align: center;
    align-items: center;
}
.mui-flex, .mui-flex *, .mui-flex:after, .mui-flex:before {
    box-sizing: border-box;
}
.mui-flex {
    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    -webkit-flex-wrap: wrap;
    -ms-flex-wrap: wrap;
    flex-wrap: wrap;
}
#s-actionBar-container .action-bar .support {
    max-width: 50px;
    border-right: 1px solid rgba(0,0,0,.05);
    min-width: 26px;
}
#s-actionBar-container .action-bar .cell {
    height: 49px;
    text-align: center;
}
#s-actionbar .cell {
    border-right: 1px solid rgba(0,0,0,.05);
}
.mui-flex>.cell {
    -webkit-box-flex: 1;
    -webkit-flex: 1;
    -ms-flex: 1;
    flex: 1;
    width: 0;
    -webkit-flex-basis: 0;
    -ms-flex-preferred-size: 0;
    flex-basis: 0;
    max-width: 100%;
    display: block;
    padding: 0!important;
    position: relative;
}
#s-actionBar-container .action-bar .cell a {
    position: absolute;
    width: 100%;
    height: 100%;
    display: block;
    top: 0;
	text-align: center;
}
#s-actionBar-container .action-bar .addfav {
    max-width: 50px;
    border-right: 1px solid rgba(0,0,0,.05);
    min-width: 26px;
}
#s-actionBar-container .action-bar .cart {
    background: #ffb401;
}
#s-actionBar-container .action-bar .buy {
    background: #ff6601;
}
#s-actionBar-container .action-bar>button {
    border: 0;
    color: #fff;
    font-size: 15px;
	font-family: inherit;
}
.icon {
    display: block;
    margin-left: auto;
    margin-right: auto;
    margin-top: 6px;
}
.support .icon {
    background-image:url(../images/ht.png);
    background-size: 18px 18px;
    width: 18px;
    height: 18px;
}
.addfav .icon {
    background-image:url(../images/xx.png);
    background-size: 18px 18px;
    width: 18px;
    height: 18px;
}

