          <mapper class="com.ssm.mapper.WRewardMapper"/> 
           <mapper class="com.ssm.mapper.DRewardMapper"/> 
      <mapper class="com.ssm.mapper.WRewardExtremeMapper"/>     
         <mapper class="com.ssm.mapper.WithDrewMapper"/>    
        <mapper class="com.ssm.mapper.PromotionMapper"/> 
     <mapper class="com.ssm.mapper.AccountSupplementMapper"/>                                
                         
                                 
                                   
  </mappers>
</configuration>